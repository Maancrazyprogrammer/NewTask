
<div class="nested-form-wrapper" data-new-record="<%= f.object.new_record? %>">
   <%= f.hidden_field :company_id, value: @company_Temp_Id %>
    <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :email %>
 <%= f.email_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.label :role %>
    <%= f.select :role, User.roles.keys %>
  
   <button type="button" data-action="nested-form#remove">Remove</button>

  <%= f.hidden_field :_destroy %>
</div>